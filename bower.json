{
  "name": "wp-starter-theme",
  "description": "A starter theme for WP developer",
  "main": "index.js",
  "authors": [
    "Thanh Hoang"
  ],
  "license": "ISC",
  "keywords": [
    "wordpress",
    "theme",
    "gulp",
    "sass",
    "browersync"
  ],
  "moduleType": [
    "amd",
    "es6",
    "globals",
    "node",
    "yui"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "animate.css": "~3.5.1",
    "bootstrap": "latest",
    "smoothscroll-for-websites": "^1.4.4",
    "jarallax": "^1.5.1",
    "owl-carousel": "^1.3.2",
    "bxslider-4": "bxSlider#~4.2.5",
    "jquery.maskedinput": "^1.4.1",
    "modernizr": "~3.3.1"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js"
      ]
    },
    "owl-carousel": {
      "main": [
        "owl-carousel/owl.carousel.css",
        "owl-carousel/owl.theme.css",
        "owl-carousel/owl.transitions.css",
        "owl-carousel/owl.carousel.js"
      ]
    }
  }
}